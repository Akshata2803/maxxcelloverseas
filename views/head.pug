mixin head
        //- meta tags
        meta(charset='utf-8')
        meta(name='viewport' content='width=device-width, initial-scale=1')
       
        //- Google Tag Manager
        script.
            (function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
